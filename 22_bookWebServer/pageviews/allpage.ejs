<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Books</title>
</head>
<body>
  <main>
    <h1>Books</h1>
    <table>
      <thead>
        <tr>
          <th>Book id</th>
          <th>Name</th>
          <th>Author</th>
          <th>Type</th>
          <th>Year of publication</th>
        </tr>
      </thead>
      <tbody>
        <% data.forEach(book => { %>
          <tr>
            <td><%= book.bookID %></td>
            <td><%= book.name %></td>
            <td><%= book.author %></td>
            <td><%= book.type %></td>
            <td><%= book.year %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <a href="/"><button>Menu</button></a>
  </main>
</body>
</html>